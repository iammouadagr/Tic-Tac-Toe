<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.menuController">
   <children>
      <Pane layoutX="1.0" layoutY="2.0" prefHeight="400.0" prefWidth="601.0">
         <children>
            <MenuBar layoutY="-2.0" prefHeight="18.0" prefWidth="600.0">
              <menus>
                <Menu mnemonicParsing="false" text="IA">
                  <items>
                    <MenuItem fx:id="configItem" mnemonicParsing="false" onAction="#configItemActionPerformed" text="Config" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </Pane>
      <Pane layoutY="27.0" prefHeight="372.0" prefWidth="601.0">
         <children>
            <Button fx:id="HvHButton" layoutX="79.0" layoutY="174.0" mnemonicParsing="false" text="with other player" />
            <Button fx:id="HvAiButton" layoutX="414.0" layoutY="174.0" mnemonicParsing="false" onAction="#LaunchHvAiButton" text="with computer" />
            <CheckBox fx:id="FacileCheckBox" disable="true" layoutX="369.0" layoutY="225.0" mnemonicParsing="false" onAction="#FacileModeAction" text="Facile" />
            <CheckBox fx:id="DifficileCheckBox" disable="true" layoutX="507.0" layoutY="225.0" mnemonicParsing="false" onAction="#DifficileModeAction" text="Difficle" />
            <Button fx:id="ContinueButton" disable="true" layoutX="275.0" layoutY="294.0" mnemonicParsing="false" onAction="#ContinueOnAction" prefHeight="25.0" prefWidth="93.0" text="Continue" />
         </children>
      </Pane>
   </children>
</AnchorPane>
